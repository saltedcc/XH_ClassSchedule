<view class='top'>
  <view class='first_k'>
    <view class='cname moren'>
      <view class='moren_22'>{{class_name}}</view>
      <view class='moren_22'>（{{class_type}}）</view>
    </view>
  </view>
  <view class='second_k'>
      <view class='ctype moren'>
        <view><view class='iconfont icon-jiaoyu' style='color:red;'></view></view>
        <view class='moren_21'>老师</view>
