      <view class='ctype moren'>
        <view><view class='iconfont icon-shijian' style='color:#e9806e;'></view></view>
        <view class='moren_21'>节数</view>
        <view class='moren_2'>{{class_cstart}}-{{class_cend}}节</view>
      </view>
